<div class="dashboard-show">
  <div class="row">
    <div class="col-sm-3">
      <div class="tab">
        <div class="tab-top">
          <h4>Categories</h4>
        </div>
        <div class="tab-bot">
          <h1><%= @categories.length %></h1>
        </div>
      </div>
    </div>
    <div class="col-sm-3">
      <%= link_to admin_products_path, class: "card" do %>
        <div class="tab-top">
          <h4>Products</h4>
        </div>
        <div class="tab-bot">
          <h1><%= @products.length %></h1>
        </div>
      <% end %>